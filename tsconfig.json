{
  "compilerOptions": {
    "target": "es2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["ES2021", "DOM"],
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "web/static/js/**/*.ts",
    "web/static/js/**/*.tsx",
    "web/static/js/**/*.d.ts"
  ]
}
